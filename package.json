{
  "name": "reformatjs",
  "version": "1.0.0",
  "description": "A node CLI designed to refactor existing codebases to support i18n with FormatJS",
  "main": "src/index.js",
  "bin": "src/index.js",
  "scripts": {
    "build": "rollup src/index.js --file dist/index.js --format cjs",
    "build-watch": "npm run build -- --watch",
    "test": "jest",
    "test-watch": "jest --watch --coverage",
    "coverage": "jest --coverage",
    "reset": "git co example-projects/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matt-t-johnson/reformatjs.git"
  },
  "keywords": [
    "node",
    "FormatJS",
    "i18n",
    "l10n",
    "internationalization",
    "localization",
    "CLI",
    "codemod",
    "refactor"
  ],
  "author": "Matt Johnson",
  "license": "ISC",
  "dependencies": {
    "@babel/generator": "^7.13.9",
    "@babel/parser": "^7.13.12",
    "@babel/traverse": "^7.13.0",
    "@babel/types": "^7.13.13",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "jest": "^27.0.4"
  }
}
